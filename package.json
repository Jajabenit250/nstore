    "@solana/web3.js": "^1.25.0",
    "@types/amqplib": "^0.8.2",
    "ajv": "^8.6.2",
    "amqplib": "^0.8.0",
    "antd": "^4.16.7",
    "antd-mobile": "^2.3.4",
    "arweave": "^1.10.15",
    "bn.js": "^5.2.0",
    "buffer": "^6.0.3",
